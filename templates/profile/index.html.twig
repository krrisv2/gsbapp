<!DOCTYPE html>

<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
   {% for key,item in userData %}
        {{item}}
   {% endfor %}

   {% if userData.Type == "2" and locataireform is defined %}
        {{ form(locataireform) }} 
         <div>
            <a href = '{{url("profile_visites")}}'>Mes Visites</a> 
        </div>
   {% endif %}
   
   {% if userData.Type == "1"%}
        <div>
            <a href = '{{url("gestion_appartements")}}'>Gestion des appartements</a> 
        </div>
   {% endif %}

    {% if errorMessage is defined %}
            <p>{{ errorMessage }}</p>
    {% endif %}
</div>

